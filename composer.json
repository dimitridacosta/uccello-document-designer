{
    "name": "uccello/document-designer",
    "description": "Convert docx files templates into PDF.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitri DA COSTA",
            "email": "dimitri.dacosta34@gmail.com"
        },
        {
            "name": "Jonathan SARDO",
            "email": "jonathan@uccellolabs.com"
        }
    ],
    "require": {
        "uccello/uccello": "1.0.*",
        "phpoffice/phpword": "^0.16.0",
        "phpoffice/phpspreadsheet": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Uccello\\DocumentDesigner\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uccello\\DocumentDesigner\\Providers\\AppServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
